{
  "name": "all-search 全搜",
  "description": "all-search 全搜，搜索引擎快捷跳转，支持任意网站展示",
  "version": "1.0.0",
  "icons": {},
  "action": {
    "default_title": "all-search"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./contentScripts/index.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "options_ui": {
    "page": "options/index.html",
    "chrome_style": true
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/*"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "manifest_version": 3
}
