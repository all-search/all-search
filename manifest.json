{
  "name": "all-search 全搜",
  "description": "all-search 全搜，搜索引擎快捷跳转，支持任意网站展示",
  "version": "1.0.0",
  "icons": {},
  "action": {
    "default_title": "all-search"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-scripts/main.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "side_panel": {
    "default_path": "sidePanel/index.html"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/*"
      ]
    }
  ],
  "permissions": [
    "storage",
    "sidePanel"
  ],
  "manifest_version": 3
}
